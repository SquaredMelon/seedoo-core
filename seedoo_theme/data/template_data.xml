<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data noupdate="0">

        <template id="seedoo_theme.web_login_layout" inherit_id="web.login_layout" name="Seedoo Login">
            <xpath expr="//div[@class='oe_single_form_footer']/a[last()]" position="replace">
                <a href="https://www.seedoo.it/r/jhF" target="_blank">Seedoo</a> <a href="https://www.odoo.com" target="_blank">is an Odoo App</a>
            </xpath>
        </template>

        <template id="seedoo_theme.web_menu_secondary" inherit_id="web.menu_secondary" name="Seedoo Menu Secondary">
            <xpath expr="//div[@class='oe_footer']" position="replace">
                <div class="oe_footer">
                    <a href="https://www.seedoo.it/r/FaH" target="_blank"><span>Seedoo</span></a> is an <a href="https://www.odoo.com" target="_blank"><span>Odoo</span></a> App
                </div>
            </xpath>
        </template>

    </data>
</openerp>